@{
	Layout = "_Layout";
}

@model List<Marqa.Domain.Entities.Setting>


<div class="container">
    <table class="table table-striped table-hover">
        <thead class="table-dark">
        <tr>
            <th>#</th>
            <th>Key</th>
            <th>Value</th>
            <th>Category</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model) {
            <tr>
                <td>@item.Id</td>
                <td>@item.Key</td>
                <td>@item.Value</td>
                <td>@item.Category</td>
            </tr>
        }
        </tbody>
    </table>
</div>
